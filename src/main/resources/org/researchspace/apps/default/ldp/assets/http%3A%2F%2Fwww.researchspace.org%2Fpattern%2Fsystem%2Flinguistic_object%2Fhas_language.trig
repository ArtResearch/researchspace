  _:genid-0195e64fd4814679af3ef786784edb3d-t4tsx8 a <http://spinrdf.org/sp#Query>;
    <http://spinrdf.org/sp#text> """DELETE {   
  $subject crm:P72_has_language $value .
  $value crm:P72i_is_language_of $subject .
}  WHERE {
  $subject crm:P72_has_language $value .
  $value crm:P72i_is_language_of $subject .
      
  ?value a crm:E56_Language .
  ?value skos:prefLabel ?label .
}""" .
  
  _:genid-0195e64fd4814679af3ef786784edb3d-ppa6r a <http://spinrdf.org/sp#Query>;
